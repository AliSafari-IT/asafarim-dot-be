{
  "name": "@asafarim/shared-ui-react",
  "version": "0.5.1",
  "private": true,
  "type": "module",
  "files": [
    "components",
    "contexts",
    "hooks",
    "index.css",
    "index.ts",
    "svg-icons",
    "package.json",
    "tsconfig.json",
    "**/*.css"
  ],
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -w",
    "start": "pnpm run build",
    "prepublishOnly": "tsc -b"
  },
  "exports": {
    ".": "./index.ts",
    "./components/*": "./components/*",
    "./contexts/*": "./contexts/*",
    "./hooks/*": "./hooks/*",
    "./svg-icons/*": "./svg-icons/*",
    "./index.css": "./index.css"
  },
  "typesVersions": {
    "*": {
      "components/*": [
        "components/*.ts",
        "components/*.tsx",
        "components/*/index.ts"
      ],
      "contexts/*": [
        "contexts/*.ts",
        "contexts/*.tsx"
      ],
      "hooks/*": [
        "hooks/*.ts",
        "hooks/*.tsx"
      ]
    }
  },
  "peerDependencies": {
    "@asafarim/react-themes": "workspace:*",
    "@asafarim/shared-i18n": "workspace:*",
    "@asafarim/shared-tokens": "workspace:*",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.0 || ^19.0.0",
    "@types/react-dom": "^18.2.0 || ^19.0.0",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-popover": "^1.1.15",
    "clsx": "^2.1.1",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^3.3.1"
  }
}
