{
  "name": "@asafarim/shared-ui-react",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "files": [
    "components",
    "contexts",
    "hooks",
    "index.css",
    "index.ts",
    "svg-icons",
    "package.json",
    "tsconfig.json"
  ],
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "tsc -b",
    "prepublishOnly": "tsc -b"
  },
  "exports": {
    ".": "./index.ts",
    "./components/*": "./components/*",
    "./contexts/*": "./contexts/*",
    "./hooks/*": "./hooks/*",
    "./svg-icons/*": "./svg-icons/*",
    "./index.css": "./index.css"
  },
  "typesVersions": {
    "*": {
      "components/*": [
        "components/*.ts",
        "components/*.tsx"
      ],
      "contexts/*": [
        "contexts/*.ts",
        "contexts/*.tsx"
      ],
      "hooks/*": [
        "hooks/*.ts",
        "hooks/*.tsx"
      ]
    }
  },
  "peerDependencies": {
    "@asafarim/shared-tokens": "workspace:*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@asafarim/react-themes": "^1.6.1",
    "react-router-dom": "^7.8.2"
  }
}
