{
    "name": "@asafarim/react-themes",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "description": "Comprehensive theme management system for React applications with dark/light mode support",
    "files": [
        "src",
        "index.ts",
        "package.json",
        "tsconfig.json",
        "README.md"
    ],
    "main": "index.ts",
    "types": "index.ts",
    "scripts": {
        "build": "tsc -b",
        "dev": "tsc -w",
        "prepublishOnly": "tsc -b"
    },
    "exports": {
        ".": "./index.ts",
        "./types": "./src/types/index.ts",
        "./themes": "./src/themes/index.ts",
        "./hooks": "./src/hooks/index.ts",
        "./components": "./src/components/index.ts"
    },
    "typesVersions": {
        "*": {
            "types": [
                "src/types/index.ts"
            ],
            "themes": [
                "src/themes/index.ts"
            ],
            "hooks": [
                "src/hooks/index.ts"
            ],
            "components": [
                "src/components/index.ts"
            ]
        }
    },
    "peerDependencies": {
        "react": "^18.2.0 || ^19.0.0",
        "react-dom": "^18.2.0 || ^19.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.3.1",
        "@types/react": "^18.2.0 || ^19.0.0",
        "@types/react-dom": "^18.2.0 || ^19.0.0",
        "typescript": "~5.8.3"
    },
    "keywords": [
        "react",
        "theme",
        "dark-mode",
        "light-mode",
        "theme-management",
        "typescript",
        "css-variables"
    ],
    "author": "ASafariM",
    "license": "CC BY 4.0"
}