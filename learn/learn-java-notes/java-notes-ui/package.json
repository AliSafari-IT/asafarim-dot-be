{
  "name": "java-notes-ui",
  "description": "Study Notes App",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "kill": "kill-port 5183",
    "build:shared": "pnpm --filter @asafarim/shared-ui-react build",
    "start": "pnpm run kill && pnpm run build:shared && pnpm run dev",
    "app": "pnpm i && concurrently \"pnpm run start\" \"cd ../java-notes-api && pnpm api\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@asafarim/react-themes": "^1.9.0",
    "@asafarim/shared-i18n": "^0.6.5",
    "@asafarim/shared-ui-react": "workspace:*",
    "@asafarim/design-tokens": "^0.5.0",
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.0",
    "@codemirror/highlight": "^0.19.8",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.11.3",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.8",
    "axios": "^1.13.1",
    "concurrently": "8.2.0",
    "fuse.js": "^7.1.0",
    "kill-port": "2.0.1",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.28.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-helmet-async": "^1.0.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
